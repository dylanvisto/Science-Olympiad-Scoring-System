<html t:type="layout" title="Event Division"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">
      
      
      
      <div class="eg">
        <t:form t:id="form" class="form-inline">
            <div class="form-group">
                Division: 
                <t:select model="divisionSelectModel" value="selectedDivision" encoder="divisionEncoder"/>
            </div>
            <div class="form-group">
                <t:submit/>
            </div>
        </t:form>
    </div>
      
      
      
</html>
      