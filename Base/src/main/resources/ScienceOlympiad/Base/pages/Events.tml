<html t:type="layout" title="Event Report"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">

    <center><h1>${event.getName()}</h1></center>
    <h1>Participants:</h1>
    <table t:type="grid" t:source="scores" t:row="score" t:include="SchoolName, Score" >
		<p:schoolNameCell>
			<a t:type="pagelink" t:page="Report/Team" t:context="${score.getSchoolId()}">${getTeamName(score)}</a>
		</p:schoolNameCell>
    </table>
	

	<a t:id="DownloadLink"> Generate Report </a>

</html>
